{
    "version": 2,
    "builds": [
        {
            "src": "createpassword.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/createpassword.html"
        }
        
    ],
    "rewrites":  [
    {
        "source": "/(.*)",
        "destination": "/"
    }
  ]
}
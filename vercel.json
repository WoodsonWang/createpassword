{
    "version": 2,
    "builds": [
        {
            "src": "createpassword.html",
            "use": "@vercel/static"
        },
        {
            "src": "sitemap.xml",
            "use": "@vercel/static"
        },
        {
            "src": "robots.txt",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/createpassword.html"
        },
        {
            "src": "/sitemap.xml",
            "dest": "/sitemap.xml",
            "headers": {
                "Content-Type": "text/xml"
        }
    },
        {
            "src": "/robots.txt",
            "dest": "/robots.txt",
            "headers": {
                "Content-Type": "text/plain"
            }
        }
        
    ]
   
  
}